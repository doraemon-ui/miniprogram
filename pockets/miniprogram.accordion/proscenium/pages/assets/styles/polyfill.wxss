@media (prefers-color-scheme: light) {
  page [data-dora-theme="auto"] button,
  page [data-dora-theme="light"] button {
    background-color: #f8f8f8;
    color: #000000;
  }
}

page [data-dora-theme="light"] button {
  background-color: #f8f8f8;
  color: #000000;
}

@media (prefers-color-scheme: dark) {
  page [data-dora-theme="auto"] button,
  page [data-dora-theme="dark"] button {
    background-color: #343434;
    color: #d6d6d6;
  }
}

page [data-dora-theme="dark"] button {
  background-color: #343434;
  color: #d6d6d6;
}
