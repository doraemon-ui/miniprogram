<view class="dora-class {{ classes.wrap }}" style="{{ containerStyle }}">
  <view class="{{ classes.hd }}" wx:if="{{ title }}">{{ title }}</view>
  <view class="{{ classes.bd }}" style="{{ internalBodyStyle }}">
    <slot></slot>
  </view>
  <view class="{{ classes.ft }}" wx:if="{{ label }}">{{ label }}</view>
</view>
