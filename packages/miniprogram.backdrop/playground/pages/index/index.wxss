.custom-content {
  position: absolute;
  top: 50%;
  left: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 160px;
  height: 160px;
  margin-top: -80px;
  margin-left: -80px;
  padding: 16px;
  background: var(--dora-component-background-color, #fff);
  border-radius: 8px;
  box-sizing: border-box;
}