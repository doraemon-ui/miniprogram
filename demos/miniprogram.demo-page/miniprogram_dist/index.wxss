@import '../../@doraemon-ui/style/var.wxss';

@font-face {
  font-family: 'DemoPage iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2');
  font-weight: 400;
  font-style: normal;
  font-display: swap
}
.dora-demo-page__iconfont {
  font-family: "DemoPage iconfont"!important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale
}
.dora-demo-page__iconfont-light::before {
  content: "\e705"
}
.dora-demo-page__iconfont-dark::before {
  content: "\e501"
}
.dora-demo-page {
  position: relative;
  min-height: 100%;
  color: rgba(0,0,0,.85);
  color: rgba(var(--dora-text-color-rgb),.85);
  background-color: #fff;
  background-color: var(--dora-background-color)
}
.dora-demo-page__hd {
  position: relative;
  padding: 80rpx
}
.dora-demo-page__bd {
  padding-bottom: 80rpx
}
.dora-demo-page__bd--spacing {
  padding-left: 30rpx;
  padding-right: 30rpx
}
.dora-demo-page__ft {
  padding-bottom: 20rpx;
  text-align: center
}
.dora-demo-page__title {
  text-align: left;
  font-size: 40rpx;
  font-weight: 400
}
.dora-demo-page__desc {
  margin-top: 10rpx;
  color: rgba(0,0,0,.45);
  color: rgba(var(--dora-text-color-rgb),.45);
  text-align: left;
  font-size: 28rpx
}
.dora-demo-page__darkmode {
  position: absolute;
  right: 24rpx;
  top: 24rpx;
  width: 48rpx;
  height: 48rpx;
  font-size: 48rpx;
  line-height: 48rpx
}